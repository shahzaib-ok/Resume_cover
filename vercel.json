
{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "devCommand": "vite --port $PORT",
  "installCommand": "npm install",
  "functions": {
  "api/*.ts": {
    "memory": 1024,
    "maxDuration": 30
  }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}